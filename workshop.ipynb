    "import numpy as np\n",
    "import matplotlib.pyplot as plt\n",
    "\n",
    "\n",
    "def load_binary(filename):\n",
    "    with open(filename, 'rb') as fp:\n",
    "        x = np.fromfile(fp, dtype='float32')\n",
    "    return x\n",
    "\n",
    "\n",
    "plt.plot(load_binary('data/test_001.bin'))\n",
    "plt.tight_layout()\n",
    "plt.show()\n",
    "print('Hello World Again!')\n",
    "\n"
   ]
